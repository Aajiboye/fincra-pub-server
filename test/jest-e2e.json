{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@fincra/trips/(.*)": "<rootDir>/../libs/trips/src/$1",
    "@fincra/trips": "<rootDir>/../libs/trips/src",
    "@fincra/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@fincra/common": "<rootDir>/../libs/common/src"
  }
}